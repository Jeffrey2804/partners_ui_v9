{
  "name": "professional-dashboard",
  "version": "1.0.0",
  "description": "A modern, responsive dashboard application for pipeline management and business analytics",
  "type": "module",
  "author": "Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/professional-dashboard.git"
  },
  "keywords": [
    "react",
    "dashboard",
    "pipeline",
    "analytics",
    "business",
    "management"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "analyze": "npm run build && npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
